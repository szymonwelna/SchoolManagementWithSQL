<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<BorderPane
        fx:id="rootPane"
        prefHeight="400.0"
        prefWidth="600.0"
        stylesheets="@/css/style.css"
        xmlns="http://javafx.com/javafx/17.0.12"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="org.example.schoolmanagementwithsql.controllers.MainScreenController">

    <center>
        <Pane
                styleClass="background-pane"
                BorderPane.alignment="CENTER"
        />
    </center>

    <bottom>
        <HBox
                styleClass="h-box"
                fx:id="bottomHBox"
                alignment="CENTER"
                BorderPane.alignment="CENTER"
        />
    </bottom>

</BorderPane>
